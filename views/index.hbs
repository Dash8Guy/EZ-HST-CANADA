                                <input type="file" class="form-control-file mt-0 pt-0" name="imgloadIncome" id="imgloadIncome"
                                  accept=".jpg, .jpeg, .png" style="opacity: 0;" />
                              </div>
                              <div class="form-group d-md-inline mt-0 pt-0" id="incomedivBelowFileInput">

                                <div class="form-check form-check-inline">
                                  <label class="form-check-label d-md-inline mr-2 mt-0 pt-0" for="checkboxReceiptIncome"
                                    id="checkboxReceiptIncomeLbl">Include
                                    Invoice
                                  </label>
                                  <input type="checkbox" data-toggle="tooltip" title="Check this box to include the invoice when you submit the form - If unchecked when you click Save Changes, this invoice, if present, will be removed."
                                    class="form-check-input d-md-inline" id="checkboxReceiptIncome">
                                  <button id="btnRemoveImgIncome" data-toggle="tooltip" title="Click to remove image. To remove invoice image from database, the form must be submitted with Include Invoice UNCHECKED"
                                    form="someOtherForm" class="btn btn-sm btn-primary ml-3" onclick="removeIncomeImage()">Remove
                                    Image</button>
                                </div>

                              </div>
                            </form>
                            <div class="container" id="incomeImgContainer">
                              <img src="" alt="" class="img-fluid w-1 h-1" id="myImgIncome">
                            </div>

                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
