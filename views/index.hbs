  <div class="modal fade" id="IncomeModal" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog modal-md">
      <div class="modal-content">
        <div class="modal-header bg-success text-white" id="incomeModalHead">
          <p class="modal-title" id="incomeTitle" style="font-size: 16px;">Business Revenue Entry Form</p>

          <form class="ml-auto" id="incomeExpForm">
            <select class="form-control bg-white text-success mySelect" id="incomeSelector" style="width: 120px; height: 24px; font-size: 16px;"
              onchange="updateIncomeHeader()">
              <option value="Business" selected>Business</option>
              <option value="Rental">Rental</option>
            </select>
          </form>

          <button class="close" onclick="hideIncomeModal()">
            <span>&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <div class="container align-middle mt-0 pt-0" id="alertContainerIncome">
          </div>

          <form id="formIncomeEntry" name="formIncomeEntry" method="POST" action="" target="#">
            <div role="form" class="row text-center" oninput="updateIncomeFormStatus()">
              <div class="col-lg-4">
                <div class="form-group">
                  <label id="IncomeDateLbl" for="incomeDate">Date</label>
                  <input id="incomeDate" form="formIncomeEntry" name="incomeDate" type="date" min="2018-01-01" max="2018-12-31"
                    class="form-control p-0 text-right" data-toggle="tooltip" title="Use Calendar or Up/Down arrows to change Year/Month/Day"
                    required>
                </div>
                <div class="form-group">
                  <label id="incomenetAmtLbl" for="incomenetAmt">Net Amount</label>
                  <div class="input-group">
                    <div class="input-group-prepend">
                      <span class="input-group-text bg-dark text-white text-left ml-0 pl-1 linkAppend form-control" id="incomeAutoAmount"
                        data-toggle="tooltip" title="Click after entering total amount including taxes to fill the tax boxes below automatically"
                        onclick="AutoFillTaxes('income')">auto</span>
                    </div>
                    <input type="number" form="formIncomeEntry" step=".01" name="incomenetAmt" id="incomenetAmt" class="form-control text-right"
                      value=0 onchange="totalUp('income')" data-toggle="tooltip" title="After entering total amount including taxes. Click auto to apply taxes in boxes below.">
                  </div>
                </div>
                <div class="form-group">
                  <label id="incomehstAmtLbl" for="incomehstAmt">HST Amount</label>
                  <input type="number" form="formIncomeEntry" step=".01" id="incomehstAmt" name="incomehstAmt" class="form-control text-right"
                    value=0 onchange="totalUp('income')" data-toggle="tooltip" title="Use auto function above or if not a standard tax rate, enter it Manually"
                    required>
                </div>
                <div class="form-group">
                  <label id="incomepstAmtLbl" for="incomepstAmt">PST Amount</label>
                  <input type="number" form="formIncomeEntry" step=".01" id="incomepstAmt" name="incomepstAmt" class="form-control text-right"
                    value=0 onchange="totalUp('income')" data-toggle="tooltip" title="Use auto function above or if not a standard tax rate, enter it Manually"
                    required>
                </div>
                <div class="form-group">
                  <label id="incomeTotalAmtLbl" for="incomeTotalAmt">Total</label>
                  <input type="number" form="formIncomeEntry" step=".01" id="incomeTotalAmt" name="incomeTotalAmt"
                    class="form-control text-right" value=0 data-toggle="tooltip" title="This displays the total of your invoice and is automatically calculated from entries above."
                    readonly>
                </div>
                <div class="form-group">
                  <label id="incomeExpIDLbl" for="incomeExpID">Revenue Status</label>
                  <input type="text" form="formIncomeEntry" id="incomeExpID" name="incomeExpID" class="form-control text-center"
                    value="NEW" data-toggle="tooltip" title="Displays the Revenue Status (NEW - SAVED - ALTERED)"
                    readonly>
                  <input type="text" form="formIncomeEntry" id="incomeBlindExpID" name="incomeBlindExpID" class="form-control d-none"
                    readonly>
                </div>
              </div>

              <div class="col-lg-8">
                <div class="form-group">
                  <label id="incomeDescriptionLbl" for="incomeDescription">Description</label>
                  <input type="text" form="formIncomeEntry" id="incomeDescription" name="incomeDescription" class="form-control text-center"
                    placeholder="Enter Brief Description" data-toggle="tooltip" title="Brief description goes here."
                    required>
                </div>

                <div class="form-group">
                  <label id="incomeAddVendorLbl" for="vendorSelectIncome">Client / Customer</label>
                  <div class="input-group">
                    <div class="input-group-prepend">
                      <span class="input-group-text bg-dark text-white text-right pr-0 m-0 linkAppend form-control" id="incomeAddVendor"
                        data-toggle="tooltip" title="Click to Add another Client/Customer" onclick="addIncomeVendor()">add</span>
                    </div>
                    <div class="input-group-prepend">
                      <span class="input-group-text bg-danger text-white text-center linkAppend form-control" id="incomeDeleteVendor"
                        data-toggle="tooltip" title="Click to Delete the Selected Client/Customer" onclick="deleteSelectedIncomeVendor()">X</span>
                    </div>
                    <select class="form-control mySelect" form="formIncomeEntry" id="vendorSelectIncome" name="vendorSelectHome"
                      data-toggle="tooltip" title="Click down arrow to select Client/Customer" required>
                      <option value="">Select or Add Client/Customer</option>
                    </select>
                  </div>
                </div>

                <div class="form-group pb-0 mb-0">
                  <label id="incomePartySelectLbl" for="incomePartySelect">Choose Party Represented</label>
                  <div class="input-group">
                    <div class="input-group-prepend">
                      <span class="input-group-text bg-dark text-white text-right pr-0 m-0 linkAppend form-control" id="incomeAddParty"
                        data-toggle="tooltip" title="Click to Add another Party Represented" onclick="addIncomeParty()">add</span>
                    </div>
                    <div class="input-group-prepend">
                      <span class="input-group-text bg-danger text-white text-center linkAppend form-control" id="incomeDeleteParty"
                        data-toggle="tooltip" title="Click to Delete the Selected Party Represented" onclick="deleteSelectedIncomeParty()">X</span>
                    </div>
                    <select class="form-control mySelect" form="formIncomeEntry" id="incomePartySelect" name="incomePartySelect"
                      data-toggle="tooltip" title="Click down arrow to select the Party Represented" required>
                      <option value="">Select Party Represented</option>
                    </select>
                  </div>
                </div>

                <div class="text-center d-md-inline pt-0 pb-0">

                  <div id="incomeExpShowHideReceiptDiv" class="container d-md-inline" data-toggle="tooltip" title="Click to toggle Invoice Functions.">
                    <button type="button" id="incomeExpShowHideReceipt" class="btn btn-sm btn-success mr-0 mt-2 d-xs-block"
                      data-toggle="collapse" href="#collapseIncome1" onclick="updateIncomeButtonText()">Show
                      Invoice Controls</button>
                  </div>
                  <div class="container d-md-inline">
                    <button type="button" id="incomeExpShowFullSize" class="btn btn-sm btn-warning mr-0 ml-0 mt-2 d-xs-block"
                      data-toggle="tooltip" title="Click to View Full Size Invoice Image." onclick="displayFullSizeIncomeImage()">Show
                      Invoice</button>
                  </div>
                </div>

                <div id="collapseIncome1" class="collapse mt-2">


                  <div class="text-dark mx-auto my-0" id="imgIncomeReceiptCollapse">
                    <div class="card">
                      <div class="card-content">
                        <div class="modal-body mt-0">
                          <div class="container">
                            <form ref='uploadIncomeForm' id='uploadIncomeForm' name="uploadIncomeForm" action='' method='post'
                              encType="multipart/form-data">
                              <div class="form-group mt-0 p-0">
                                <label id="incomeFileInputLbl" class="mb-0" for="imgloadIncome" data-toggle="tooltip"
                                  title="Click to Select Invoice Image.">Choose
                                  Invoice
                                  images(PNG,JPG)</label>
                                <input type="file" class="form-control-file mt-0 pt-0" name="imgloadIncome" id="imgloadIncome"
                                  accept=".jpg, .jpeg, .png" style="opacity: 0;" />
                              </div>
                              <div class="form-group d-md-inline mt-0 pt-0" id="incomedivBelowFileInput">

                                <div class="form-check form-check-inline">
                                  <label class="form-check-label d-md-inline mr-2 mt-0 pt-0" for="checkboxReceiptIncome"
                                    id="checkboxReceiptIncomeLbl">Include
                                    Invoice
                                  </label>
                                  <input type="checkbox" data-toggle="tooltip" title="Check this box to include the invoice when you submit the form - If unchecked when you click Save Changes, this invoice, if present, will be removed."
                                    class="form-check-input d-md-inline" id="checkboxReceiptIncome">
                                  <button id="btnRemoveImgIncome" data-toggle="tooltip" title="Click to remove image. To remove invoice image from database, the form must be submitted with Include Invoice UNCHECKED"
                                    form="someOtherForm" class="btn btn-sm btn-primary ml-3" onclick="removeIncomeImage()">Remove
                                    Image</button>
                                </div>

                              </div>
                            </form>
                            <div class="container" id="incomeImgContainer">
                              <img src="" alt="" class="img-fluid w-1 h-1" id="myImgIncome">
                            </div>

                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                  <!-- onchange="updateBusinessFormStatus()" -->

                </div>


                <div class="form-group text-center pt-3">
                  <div>
                    <label id="monthlyIncomeLbl" class="d-md-inline-block" for="reoccuringIncome">Monthly</label>
                    <div id="reoccuringIncome" class="border border-primary rounded d-md-inline pt-2 pl-2">

                      <div class="form-check form-check-inline">
                        <input class="form-check-input radiobtn mr-1" id="incomeExpReoccurringYES" form="formIncomeEntry"
                          type="radio" name="yesno" value="YES" data-toggle="tooltip" title="When YES is selected, multiple entries will be saved on the same day of every Month of the year from the input date month till December.">
                        <label class="form-check-label" id="monthlyIncomeLblYES" for="incomeExpReoccurringYES">YES</label>
                      </div>
                      <div class="form-check form-check-inline">
                        <input class="form-check-input radiobtn ml-1 mr-1" id="incomeExpReoccurringNO" form="formIncomeEntry"
                          type="radio" name="yesno" value="NO" data-toggle="tooltip" title="When NO is selected, only one entry will be saved on the input date."
                          checked>
                        <label class="form-check-label" id="monthlyIncomeLblNO" for="incomeExpReoccurringNO">NO</label>
                      </div>

                    </div>
                  </div>
                </div>

                <div class="mx-auto text-center">
                  <button type="button" id="incomeEntryformReset" form="formIncomeEntry" class="btn btn-sm btn-outline-primary ml-1 mt-2"
                    data-toggle="tooltip" title="When Status is NEW or SAVED, empties all inputs - When status is ALTERED, fills form with Saved values."><i
                      class="fas fa-sync-alt"></i>
                    Reset
                  </button>
                  <button type="button" id="incomeExpDeleteBtn" class="btn btn-sm btn-outline-danger ml-1 mt-2" onclick="deleteIncomeExpense()"
                    data-toggle="tooltip" title="Click to Delete current Revenue"><i class="far fa-trash-alt"></i>
                    Delete
                  </button>

                  <button type="button" id="closeIncomeExpModal" class="btn btn-sm btn-outline-primary ml-1 mt-2"
                    data-toggle="tooltip" title="Close form." onclick="hideIncomeModal()"><i class="far fa-window-close"></i>
                    Close
                  </button>

                  <button type="button" id="incomeExpSaveChangesBtn" class="btn btn-sm btn-outline-success ml-1 mt-2"
                    data-toggle="tooltip" title="Click to Save changes you have made to an existing revenue - Revenue Status must be ALTERED!"
                    onclick="updateIncome($('#incomeSelector').val())"><i class="fas fa-save"></i>
                    Save Changes
                  </button>

                  <button type="button" id="incomeExpBtn" form="formIncomeEntry" class="btn btn-sm btn-success ml-1 mt-2"
                    data-toggle="tooltip" title="Click to add New Revenue"><i class="far fa-save"></i> Submit</button>
                </div>

